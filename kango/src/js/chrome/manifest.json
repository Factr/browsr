{
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "all_frames": true,
            "js": [
                "kango/utils.js",
                "kango/invoke_async.js",
                "kango/storage_sync.js",
                "kango/message_target.js",
                "includes/content_kango.js",
                "includes/content_init.js"
            ],
            "run_at": "document_start"
        }
    ],
    "name": "Kango",
    "icons": {
        "128": "icons/icon128.png",
        "16": "icons/button.png",
        "48": "icons/icon48.png"
    },
    "browser_action": {
        "default_icon": "icons/button.png",
        "default_title": "Kango",
        "default_popup": "popup.html"
    },
    "version": "1.0.0",
    "background": {
        "page": "background.html"
    },
    "permissions": [
        "tabs",
        "http://*/*",
        "https://*/*",
        "contextMenus",
        "webNavigation",
        "notifications",
        "cookies",
        "nativeMessaging",
        "https://www.linkedin.com/oauth/v2/authorization",
        "identity"
    ],
    "update_url": "",
    "homepage_url": "https://factr.com",
    "description": "Extension description",
    "options_page": "options.html",
    "content_security_policy": "script-src 'self' 'unsafe-eval' 'sha256-0tzuyoOqrEzlYLfmTeCY7N8QSpfH7297OSW0APcCB4s='; object-src 'self'",
    "web_accessible_resources": ["res/*", "assets/*", "icons/*"],
    "oauth2": {
        "client_id": "419144511656-rlb3lstamkhbu2tpha2n3e77o3pracu7.apps.googleusercontent.com",
        "scopes": [
            "email",
            "profile"
        ]
    }
}